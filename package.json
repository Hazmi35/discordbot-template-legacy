{
  "name": "discordbot-template-legacy",
  "version": "0.1.0",
  "private": true,
  "description": "A Discord bot template made in TypeScript with discord.js",
  "repository": "https://github.com/Hazmi35/discordbot-template-legacy",
  "license": "AGPL-3.0",
  "author": "Hazmi35 <contact@hzmi.xyz>",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "start:dev": "ts-node -r dotenv/config src/bot.ts"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@hazmi35/eslint-config/typescript"
  },
  "dependencies": {
    "@discordjs/voice": "^0.6.0",
    "date-fns": "^2.24.0",
    "discord.js": "^13.1.0",
    "got": "^11.8.2",
    "tslib": "^2.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^6.0.2",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.6.0",
    "bufferutil": "^4.0.3",
    "erlpack": "github:hzmifork/erlpack",
    "libsodium-wrappers": "^0.7.9",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
